{
  "name": "wdio_test",
  "version": "1.0.0",
  "description": "This is an assignment project",
  "main": "index.js",
  "scripts": {
    "chrome": "SET BROWSER=chrome&&wdio ./config/wdio.browser.conf.js",
    "firefox": "SET BROWSER=firefox&&wdio ./config/wdio.browser.conf.js",
    "ie": "SET BROWSER=internet explorer&&wdio ./config/wdio.browser.conf.js",
    "cross-browser": "wdio ./config/wdio.crossbrowser.conf.js",
    "report:generate": "npx allure generate --clean ./test-report/allure-result/ -o ./test-report/allure-report",
    "report:open": "npx allure open test-report/allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AutomationReddy/wdio_test.git"
  },
  "keywords": [
    "wdio",
    "webdriverio",
    "cucumber",
    "bdd"
  ],
  "author": "Vinod Reddy",
