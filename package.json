  "bugs": {
    "url": "https://github.com/AutomationReddy/wdio_test/issues"
  },
  "homepage": "https://github.com/AutomationReddy/wdio_test#readme",
  "dependencies": {
    "@wdio/cli": "^7.5.3",
    "@wdio/sync": "^7.5.3",
    "dotenv": "^8.5.0",
    "faker": "^5.5.3"
  },
  "devDependencies": {
    "@wdio/allure-reporter": "^7.5.3",
    "@wdio/cucumber-framework": "^7.5.3",
    "@wdio/local-runner": "^7.5.5",
    "@wdio/selenium-standalone-service": "^7.5.3",
    "@wdio/spec-reporter": "^7.5.3"
  }
